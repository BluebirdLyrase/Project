         <command
               commandId="StackOverFlow.commands.SearchCommand"
               icon="images/stackoverflow16px.png"
               id="StackOverFlow.menus.popupSearch"
               label="Search from Stack"
               mnemonic="M1+P"
               tooltip="Do something with this project">

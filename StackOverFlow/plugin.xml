<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="StackOverFlow.commands.StackOverFlowCategory"
            id="StackOverFlow.commands.HistoryCommand"
            name="History Command">
      </command>
      <command
            categoryId="StackOverFlow.commands.StackOverFlowCategory"
            id="StackOverFlow.commands.SearchCommand"
            name="Search Command">
      </command>
      <category
            id="StackOverFlow.commands.StackOverFlowCategory"
            name="StackOverFlowCategory">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="stackoverflow.handlers.HistoryHandler"
            commandId="StackOverFlow.commands.HistoryCommand">
      </handler>
      <handler
            class="stackoverflow.handlers.SearchHandler"
            commandId="StackOverFlow.commands.SearchCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="StackOverFlow.commands.HistoryCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
      <key
            commandId="StackOverFlow.commands.SearchCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="StackOverFlow.menus.StackOverFlowMenu"
               label="StackOverFlow"
               mnemonic="M">
            <command
                  commandId="StackOverFlow.commands.HistoryCommand"
                  id="StackOverFlow.menus.HistoryCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="StackOverFlow.toolbars.HistoryCommand">
            <command
                  id="StackOverFlow.toolbars.HistoryCommand"
                  commandId="StackOverFlow.commands.HistoryCommand"
                  icon="images/stackoverflow16px.png"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="StackOverFlow.menus.StackOverFlowMenu"
               label="StackOverFlow"
               mnemonic="M">
            <command
                  commandId="StackOverFlow.commands.SearchCommand"
                  id="StackOverFlow.menus.SearchCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="StackOverFlow.toolbars.SearchToolbar">
            <command
                  commandId="StackOverFlow.commands.SearchCommand"
                  icon="images/stackoverflow16px.png"
                  id="StackOverFlow.toolbars.SearchCommand"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="StackOverFlow"
            class="stackoverflow.ViewAndDialog.SearchResultView"
            icon="images/stackoverflow16px.png"
            id="stackoverflow.ViewAndDialog.SearchResultView"
            inject="true"
            name="SearchResult View">
      </view>
      <category
            id="StackOverFlow"
            name="StackOverFlow Category">
      </category>
      <view
            category="StackOverFlow"
            class="stackoverflow.ViewAndDialog.ContentView"
            icon="images/stackoverflow16px.png"
            id="stackoverflow.ViewAndDialog.ContentView"
            inject="true"
            name="Content View">
      </view>
      <view
            category="StackOverFlow"
            class="stackoverflow.ViewAndDialog.SearchingHistoryView"
            icon="icons/sample.png"
            id="stackoverflow.ViewAndDialog.SearchingHistoryView"
            inject="true"
            name="SearchingHistoryView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="stackoverflow.ViewAndDialog.SearchResultView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="stackoverflow.ViewAndDialog.ContentView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
         <perspectiveExtension
               targetID="org.eclipse.jdt.ui.JavaPerspective">
         </perspectiveExtension>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="stackoverflow.ViewAndDialog.SearchingHistoryView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>

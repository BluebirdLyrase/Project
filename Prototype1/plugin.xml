<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            id="Prototype1.commands.getoneAnswerCategory"
            name="get one Answer">
      </category>
      <command
            categoryId="Prototype1.commands.getoneAnswerCategory"
            name="get one Answer"
            id="Prototype1.commands.getoneAnswer">
      </command>
      <command
            categoryId="Prototype1.commands.NewViewCategory"
            id="Prototype1.commands.NewViewCommand"
            name=" NewView Command">
      </command>
      <category
            id="Prototype1.commands.NewViewCategory"
            name="NewView Category">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="prototype1.handlers.PrototypeHandler"
            commandId="Prototype1.commands.getoneAnswer">
      </handler>
      <handler
            class="prototype1.handlers.NewViewHandler"
            commandId="Prototype1.commands.NewViewCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="Prototype1.commands.getoneAnswer"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6">
      </key>
      <key
            commandId="Prototype1.commands.NewViewCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="Prototype1.menus.sampleMenu"
               label="Test StackOverflos API"
               mnemonic="M">
            <command
                  commandId="Prototype1.commands.getoneAnswer"
                  id="Prototype1.menus.getoneAnswer"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="Prototype1.toolbars.sampleToolbar">
            <command
                  id="Prototype1.toolbars.getoneAnswer"
                  commandId="Prototype1.commands.getoneAnswer"
                  icon="icons/sample.png"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="Prototype1.menus.sampleMenu"
               label="Test StackOverflos API"
               mnemonic="M">
            <command
                  commandId="Prototype1.commands.NewViewCommand"
                  id="Prototype1.menus.NewViewCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="Prototype1.toolbars.sampleToolbar">
            <command
                  commandId="Prototype1.commands.sampleCommand"
                  icon="icons/sample.png"
                  id="Prototype1.toolbars.sampleCommand"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="NewViewsCategory"
            name="NewViews Category">
      </category>
      <view
            category="NewViewsCategory"
            class="prototype1.NewViews.NewViews"
            icon="icons/sample.png"
            id="prototype1.NewViews.NewViews"
            inject="true"
            name="New Views">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="prototype1.NewViews.NewViews"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
